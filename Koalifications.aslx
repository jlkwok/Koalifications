    <start type="script">
      msg ("You wake and your beloved forest home has been destroyed. Where once was beautiful eucalyptus forest, now lies the concrete jungle. Urbanization has struck again, and the new city, Eucatown, has replaced your home. Your belly grumbles; you are hungry and longing for your favorite food, eucalyptus bread.")
      MoveObject (Koala, Alley)
    </start>
    <object name="Koala">
      <inherit name="editor_object" />
      <isDisguised type="boolean">false</isDisguised>
      <pickpocket type="int">0</pickpocket>
      <talkJeremy type="boolean">false</talkJeremy>
    </object>
    <object name="Zookeeper Uniform">
      <inherit name="editor_object" />
      <inherit name="wearable" />
      <feature_wearable />
      <wear_slots type="stringlist">
        <value>body</value>
      </wear_slots>
      <changedworn type="script">
        if (Zookeeper Uniform.worn = true) {
          set (Koala, "isDisguised", true)
        }
        else {
          set (Koala, "isDisguised", false)
        }
      </changedworn>
    </object>
  </game>
  <object name="Fifth Avenue">
    <inherit name="editor_room" />
    <exitslistprefix>You can go to </exitslistprefix>
    <descprefix>You are in</descprefix>
    <beforeenter type="script">
      msg ("Fifth Avenue seems like a poppin part of the city where some very important koalification resources could be found. You can smell the enticing scent of eucalyptus and bread coming from the nearby bakery; your mouth waters and your tummy grumbles. There is a tall skyscraper with lots of passerby going in and out of the entrance. The alley is somewhat smelly and you can see the Detective poking his head out. Finally, there is the Eucaway where everyone seems to be going to get to other places in the city.")
    </beforeenter>
    <enter type="script">
      if (Koala.isDisguised = false) {
        msg ("The public notices that you are a wild animal and a concerned individual calls animal control. You are captured and sent to the zoo to live in koala prison until you die.")
        finish
      }
    </enter>
    <object name="Bakery">
      <inherit name="editor_room" />
      <visible type="boolean">false</visible>
      <description type="string"></description>
      <descprefix>You are in </descprefix>
      <enter type="script">
        if (Koala.isDisguised = false) {
          msg ("The public notices that you are a wild animal and a concerned individual calls animal control. You are captured and sent to the zoo to live in koala prison until you die.")
          finish
        }
      </enter>
      <beforeenter type="script">
        msg ("You step into the Bakery and are immediately overcome by the enticing smell of baked eucalyptus bread. A Baker wearing a chefs hat and apron stands behind the counter moving baked goods around. The customer seems to be pondering whether or not they should purchase something. There's a back door that leads to the kitchen that is marked employees only.")
      </beforeenter>
      <object name="Baker">
        <inherit name="editor_object" />
        <inherit name="female" />
        <speak type="script">
          if (Chef Hat.worn = true) {
            if (Apron.worn = true) {
              msg ("The Baker says to you \"Hey get back in the kitchen!\" She must think you work at the Bakery.")
            }
          }
          else {
            msg ("Hi. I'm the Baker. Bread costs $5 and Eucabread is $10 if you want to buy it. If you are looking for a job, I am always willing to hire anyone who manages to put on a chefs hat and an apron.")
          }
        </speak>
        <buybread type="script"><![CDATA[
          if (Koala.money >=5) {
            AddToInventory (Bread)
            msg ("The Baker gives you a fresh loaf of bread.")
          }
          else {
            msg ("You do not have enough money to buy bread.")
          }
        ]]></buybread>
        <buyeucabread type="script"><![CDATA[
          if (Koala.money >=10  ) {
            AddToInventory (Eucabread)
            msg ("The Baker gives you a fresh loaf of eucabread and you win the game! UwU")
            finish
          }
          else {
            msg ("You do not have enough money to buy eucabread.")
          }
        ]]></buyeucabread>
        <object name="Eucabread">
          <inherit name="editor_object" />
        </object>
        <object name="Bread">
          <inherit name="editor_object" />
          <price type="int">0</price>
          <look>Yay non-eucalyptus carbsss! You can try to eat it, but you'll have the poops and die X_X</look>
          <eat type="script">
            msg ("You should have checked the bread out before eating it. You can only eat the eucabread #deathByPoops")
            finish
          </eat>
        </object>
      </object>
      <object name="Customer">
        <inherit name="editor_object" />
        <inherit name="male" />
        <speak>The bread here is so expensive.</speak>
        <ask type="scriptdictionary">
          <item key="bread">
            msg ("I wish I had some of that bread. It's made with real eucalyptus. Perfect food for any koala that's trying to blend in by wearing human clothing. Too bad I'm not a koala trying to blend in by wearing human clothing.")
          </item>
          <item key="price">
            msg ("The normal bread for human people is $5, but the special eucalyptus bread for human people and koala bears is $10.")
          </item>
        </ask>
      </object>
      <exit to="Fifth Avenue">
        <scenery type="boolean">false</scenery>
      </exit>
      <object name="Kitchen">
        <inherit name="editor_room" />
        <visible type="boolean">false</visible>
        <beforeenter type="script">
          msg ("The Kitchen is small and dingy, but seems well used. It is very hot as the oven is in use.")
        </beforeenter>
        <exit to="Bakery" />
        <object name="Oven">
          <inherit name="editor_object" />
          <look>Tis hot</look>
          <displayverbs type="stringlist">
            <value>Look at</value>
          </displayverbs>
          <bakeeucabread type="script">
            if ((Got(Bread) or Got(Bread1) or Got(Bread2)) and (Got(Eucalyptus Tree) or Got(Eucalyptus Tree1))) {
              msg ("You got the Eucabread and you win!")
              finish
            }
            else {
              msg ("You need bread and a eucalyptus tree in order to bake eucabread!")
            }
          </bakeeucabread>
        </object>
      </object>
      <exit name="Kitchen Door" to="Kitchen">
