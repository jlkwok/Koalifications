  <object name="Eucaway">
    <inherit name="editor_room" />
    <enter type="script">
      if (Koala.isDisguised = false) {
        msg ("The public notices that you are a wild animal and a concerned individual calls animal control. You are captured and sent to the zoo to live in koala prison until you die.")
        finish
      }
    </enter>
    <beforeenter type="script">
      msg ("Yike it smells like pee, but if you have a eucapass, you can go anywhere in the city. A police officer keeps watch while a mother and her kids scramble to catch the next train. There is a college student who looks like he hit the eucaganja.")
    </beforeenter>
    <object name="Police Officer">
      <inherit name="editor_object" />
      <inherit name="male" />
      <look>You can see the officer's badge which reads EPD (Eucatown Police Department).</look>
      <speak type="script">
        if (kids tshirt.worn = true) {
          if (not Got(Eucapass)) {
            msg ("You tap the police officer on the shoulder. He asks for where your parents are. You tell him that they told you they would be right back, but never did.The police officer gives you a eucapass.")
            AddToInventory (Eucapass)
          }
          else {
            msg ("The police officer looks at you funny.")
          }
        }
        else {
          msg ("If you see any kids without eucapasses send them my way.")
        }
      </speak>
    </object>
    <object name="Mother">
      <inherit name="editor_object" />
      <inherit name="female" />
      <look>She seems frazzled and overworked.</look>
      <speak type="script">
        if (kids tshirt.worn = true) {
          if (not Got(Eucapass)) {
            msg ("You tap the mother on the shoulder and she turns and asks you  where your parents are. You say that they have left you and the concerned mother gives you a eucapass")
            AddToInventory (Eucapass)
          }
          else {
            msg ("The mother looks at you funny.")
          }
        }
        else {
          msg ("I get so sad when I see kids are stuck without any eucapasses.")
        }
      </speak>
      <object name="Eucapass">
        <inherit name="editor_object" />
        <look>You can go anywhere with this pass!</look>
        <inventoryverbs type="stringlist">
          <value>Look at</value>
          <value>Use</value>
        </inventoryverbs>
      </object>
    </object>
    <object name="boy">
      <inherit name="editor_object" />
      <inherit name="male" />
      <speak>Hi friend! I'm going to school with my mom and my sister!</speak>
      <look>He has brown hair.</look>
      <take type="boolean">false</take>
    </object>
    <object name="girl">
      <inherit name="editor_object" />
      <inherit name="female" />
      <speak>Hi friend! I'm going to school with my mom and my brother!</speak>
      <look>She has curly blonde hair.</look>
    </object>
    <object name="College Student">
      <inherit name="editor_object" />
      <inherit name="male" />
      <speak><![CDATA[Hey kid, I've been working at a pizza shop, but the boss makes me work this <b>job</b> too much!]]></speak>
